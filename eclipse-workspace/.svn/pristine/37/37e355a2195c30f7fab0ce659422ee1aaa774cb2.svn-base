<script type="text/javascript">
    $(document).ready(function() {
        $('.tab_goods_2 .goods_tab_tit li').click(function(e){
            e.preventDefault();
            $(this).addClass('on').siblings().removeClass('on');	//탭버튼에 on클래스 주기
//                if ($('.tab_goods_2 .goods_tab_cont > .goods_tab_box').eq($(this).index()).length == 0) {
//                    $('.tab_goods_2 .goods_tab_cont > .goods_tab_box').removeClass('on');
//                }
            $('.tab_goods_2 .goods_tab_cont > .goods_tab_box').eq($(this).index()).addClass('on').siblings().removeClass('on');	//해당 탭 내용 열고 나머지 닫기
        });

		// 할인율 계산
		$(".item_cont").each(function(){
			var $this_del = $(this).find("del").length;
			var $this_del_price = $(this).find("del").text();
			if($this_del > 0 && $this_del_price != ""){
				var del_price1 = $(this).find("del").text().replace("원", "");
				var del_price2 = del_price1.replace(/,/g, '') * 1;
				var last_price1 = $(this).find(".item_price").children("span").text().replace("원", "");
				var last_price2 = last_price1.replace(/,/g, '') * 1;
				var price_sum = ((last_price2/del_price2)*100)*1;
				var price_per = Math.round(100-price_sum)+"%"
				$(this).find(".price_percent").text(price_per);
			}
		});
    });
</script>
