package dto;

/*
 이름      널?       유형           
------- -------- ------------ 
C_NO    NOT NULL NUMBER(4)    - 장바구니 번호
ID      NOT NULL VARCHAR2(20) - 회원 아이디
P_NO    NOT NULL NUMBER(4)    - 상품코드
C_COUNT NOT NULL NUMBER(2)    - 장바구니 수량
 */

public class CartDTO  {
	private int cNo;
	private String id;
	private int pNo;
	private String cLettering;
	private int cCount;
	
	public CartDTO() {
		// TODO Auto-generated constructor stub
	}

	public int getcNo() {
		return cNo;
	}

	public void setcNo(int cNo) {
		this.cNo = cNo;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public int getpNo() {
		return pNo;
	}

	public void setpNo(int pNo) {
		this.pNo = pNo;
	}

	public String getcLettering() {
		return cLettering;
	}

	public void setcLettering(String cLettering) {
		this.cLettering = cLettering;
	}

	public int getcCount() {
		return cCount;
	}

	public void setcCount(int cCount) {
		this.cCount = cCount;
	}
	
	
	


	

}
